{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [{
            "type": "java",
            "name": "CodeLens (Launch) - PermutationUnique",
            "request": "launch",
            "mainClass": "mine.knowledge.stack_pop_sequence.PermutationUnique",
            "projectName": "LeetCode"
        },
        {
            "type": "java",
            "name": "CodeLens (Launch) - Permutation2",
            "request": "launch",
            "mainClass": "mine.knowledge.stack_pop_sequence.Permutation2",
            "projectName": "LeetCode"
        },
        {
            "type": "java",
            "name": "CodeLens (Launch) - Subsets",
            "request": "launch",
            "mainClass": "mine.leetcode.Subsets",
            "projectName": "LeetCode"
        },
        {
            "type": "java",
            "name": "Cur File With Filter",
            "request": "launch",
            "mainClass": "${file}",
            "console": "internalConsole",
            "stepFilters": {
                "classNameFilters": [
                    "java.*",
                    "javax.*",
                    "com.sun.*",
                    "sun.*",
                    "sunw.*",
                    "org.omg.*"
                ],
                "skipSynthetics": false,
                "skipStaticInitializers": false,
                "skipConstructors": false
            }
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "internalConsole",
            "justMyCode": false
        },
        {
            "name": "GDB",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}\\out\\${fileBasenameNoExtension}.o",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "miDebuggerPath": "C:\\MinGW\\bin\\gdb.exe",
            "setupCommands": [{
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }]
        },
        {
            "name": "GDB With Build",
            "preLaunchTask": "build",
            "type": "cppdbg",
            "request": "launch",
            "program": "${fileDirname}\\${fileBasenameNoExtension}.o",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "externalConsole": true,
            "environment": [],
            "MIMode": "gdb",
            "miDebuggerPath": "C:\\MinGW\\bin\\gdb.exe",
            "setupCommands": [{
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }]
        }
    ]
}